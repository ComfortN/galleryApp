{
  "expo": {
    "name": "galleryApp",
    "slug": "galleryApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": { 
      "supportsTablet": true, 
      "infoPlist": { "NSLocationWhenInUseUsageDescription": 
      "This app uses your location to tag images with their location.", 
      "NSCameraUsageDescription": "This app uses your camera to take pictures.", 
      "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to select images." 
      } 
      }, 
      "android": { 
        "adaptiveIcon": { 
          "foregroundImage": "./assets/images/splash.png", 
          "backgroundColor": "#ffffff" }, 
          "package": "com.comfortn.galleryApp", 
          "permissions": [ 
            "ACCESS_FINE_LOCATION", 
            "CAMERA", 
            "READ_EXTERNAL_STORAGE", 
            "WRITE_EXTERNAL_STORAGE" 
            ] 
          },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/splash.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f6383485-204b-4fd7-8782-98b612a158b2"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/f6383485-204b-4fd7-8782-98b612a158b2"
    }
  }
}
